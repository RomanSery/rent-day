  "scripts": {
    "dev": "concurrently -p name -c \"yellow,magenta,blue\" -n \"webpack-server,nodemon-server,CRA\" \"npm run-script dev:server:webpack\" \"npm run-script dev:server:nodemon\" \"npm run-script dev:client\"",
    "dev:client": "react-scripts start",
    "dev:server": "concurrently -p name -c \"yellow,magenta\" -n \"webpack-server,nodemon-server\" \"npm run-script dev:server:webpack\" \"npm run-script dev:server:nodemon\"",
    "dev:server:webpack": "webpack --config webpack.config.server.js --watch",
    "dev:server:nodemon": "wait-on -l build/server.js && nodemon build/server.js",
    "build:client": "react-scripts build",
    "build:server": "webpack --config webpack.config.server.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
